{
  "version": 2,
  "builds": [
    { "src": "Back-End/**", "use": "@vercel/node" },
    { "src": "**/*.html", "use": "@vercel/static" },
    { "src": "**/*.css", "use": "@vercel/static" },
    { "src": "**/*.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },  // Replace "vercel/pathfs" with "filesystem"
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
